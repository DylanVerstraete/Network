    "eslint": "^5.16.0",
    "iltorb": "^2.4.3",
    "is-json": "*",
    "is-valid-path": "^0.1.1",
    "json-stable-stringify": "*",
    "node-watch": "^0.6.2",
    "sodium-native": "^2.4.2",
    "truncate-utf8-bytes": "^1.0.2"
  },
