{
  "name": "Sentivate",
  "version": "1.0.5",
  "main": "main.js",
  "description": "Sentivate Network Client, Server Module, UDSP, & certificate generation. This serves as a network & browser prototype.",
  "scripts": {
    "client": "nodemon client/index.js --watch client/",
    "compressionTest": "nodemon scripts/compression.js --watch scripts/compression.js",
    "cc": "nodemon ./client/createCertificate/index.js --watch ./client/createCertificate/",
    "server": "nodemon ./serverApp/server.js --watch ./server/ --watch ./serverApp/",
    "dis": "nodemon ./DIS/index.js --watch ./server/ --watch ./DIS/",
    "simulate": "nodemon ./scripts/simulate.js --watch ./client/ --watch ./server/ --watch ./scripts/simulate.js --watch utilities/",
    "certificates": "nodemon ./scripts/certificates.js --watch ./scripts/certificates.js --watch ./certificates/ --watch ./utilities/",
    "start": "electron ."
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "author": "ARITY",
  "company": "ARITY",
  "dependencies": {
    "@babel/helpers": "^7.5.5",
    "Lucy": "^2.2.3",
    "babel-eslint": "^10.0.3",
    "chalk": "*",
    "eslint": "^6.3.0",
    "iltorb": "*",
    "is-valid-path": "*",
    "node-watch": "*",
    "sodium-native": "^2.4.5",
    "what-the-pack": "^2.0.3"
  },
  "license": "ISC",
  "devDependencies": {
    "auto-changelog": "^1.16.0",
    "docredux": "^2.0.6",
    "electron": "^6.0.7",
    "rollup": "^1.20.3",
    "rollup-plugin-babel-minify": "*",
    "testatron": "^2.0.5"
  }
}
